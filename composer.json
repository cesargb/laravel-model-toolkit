{
    "name": "cesargb/laravel-model-toolkit",
    "description": "List all models, detect and delete orphaned morph relation records, and prune Eloquent models.",
    "type": "library",
    "keywords": ["laravel", "prune", "morph", "cleaner"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cesargb\\ModelToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesargb\\ModelToolkit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4",
        "illuminate/console": "^12.0|^13.0",
        "illuminate/database": "^12.0|^13.0",
        "illuminate/support": "^12.0|^13.0",
        "symfony/finder": "^7.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0|^11.0",
        "phpunit/phpunit": "^12.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesargb\\ModelToolkit\\ModelToolkitServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse --level=max src tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
